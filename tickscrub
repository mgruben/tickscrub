tickscrub() {
        sed -e 's/.span class/\n&/g' -e 's/.tr class/\n&/g' $1/view | grep 'sectionNa\|ticketRow\|ticketQt\|deliveryMet\|\"q\"\|aisle\|parking\|wheelchair\|piggyback\|obstructe' | sed '/\$/ a\ ' | sed -e 's/ EID//g' -e 's/.\/span.*//g' -e 's/^<span class=//g' -e 's/>/,/g' -e 's/\$//g' -e 's/.title*.*/,1/g' -e 's/$/;/g' | tr '\n' ' ' | sed -e 's/  ; /\n/g' | sed -e '/wheelchair/ s/$/ 1;/' -e '/wheelchair/ !s/$/ 0;/' -e '/parking/ s/$/ 1;/' -e '/parking/ !s/$/ 0;/' -e '/aisle/ s/$/ 1;/' -e '/aisle/ !s/$/ 0;/' -e '/obstructed/ s/$/ 1;/' -e '/obstructed/ !s/$/ 0;/' -e '/piggyback/ s/$/ 1;/' -e '/piggyback/ !s/$/ 0;/' -e 's/"wheelchair",1; //' -e 's/"parking",1; //' -e 's/"aisle",1; //' -e 's/"obstructed",1; //' -e 's/"piggyback",1; //' | perl -pe 's|\".*?,|\1|' | perl -pe 's|\".*?,|\1|' | perl -pe 's|\".*?,|\1|' | perl -pe 's|\".*?,|\1|' | perl -pe 's|\".*?,|\1|' > $1/finalOutput
}

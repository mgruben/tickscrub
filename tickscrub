tickscrub() {
        sed -e 's/.span class/\n&/g' -e 's/.tr class/\n&/g' $1/view | grep 'sectionNa\|ticketRow\|ticketQt\|deliveryMet\|\"q\"\|aisle\|parking\|wheelchair' | sed '/\$/ a\ ' | sed -e 's/ EID//g' -e 's/.\/span.*//g' -e 's/^<span class=//g' -e 's/>/,/g' -e 's/\$//g' -e 's/.title*.*/,1/g' > $1/finalOutput
}
